{
    "name": "pod-point/laravel-cognito-auth",
    "description": "An authentication driver for Laravel",
    "keywords": ["php", "laravel", "auth", "aws", "cognito"],
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "illuminate/config": "5.*",
        "illuminate/support": "5.*",
        "illuminate/auth": "5.*",
        "aws/aws-sdk-php": "^3.27"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Speakman",
            "email": "ben.speakman@pod-point.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PodPoint\\LaravelCognitoAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
